    <element ref='t:FECHACONCESION' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHAABANDONO' minOccurs='0' maxOccurs='1'/>
    <element ref='t:REFERENCIA' minOccurs='0' maxOccurs='1'/>
    <element ref='t:MOTIVOCAMBIOFECHA' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDPATENTE' type='string'>
 </element>

