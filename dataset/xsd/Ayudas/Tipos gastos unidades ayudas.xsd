<schema
  xmlns='http://www.w3.org/2000/10/XMLSchema'
  targetNamespace='http://www.um.es/hercules'
  xmlns:t='http://www.um.es/hercules'>

 <element name='main'>
  <complexType>
   <sequence>
    <element ref='t:DATA_RECORD' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
  </complexType>
 </element>

 <element name='DATA_RECORD'>
  <complexType>
   <sequence maxOccurs='unbounded'>
    <element ref='t:IDUNIDADAYUDA'/>
    <element ref='t:IDTIPOGASTOAYUDA'/>
    <element ref='t:DESCRIPCION'/>
    <element ref='t:PERMITIDO'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDUNIDADAYUDA' type='string'>
 </element>

 <element name='IDTIPOGASTOAYUDA' type='string'>
 </element>

 <element name='DESCRIPCION' type='string'>
 </element>

 <element name='PERMITIDO' type='string'>
 </element>
</schema>
