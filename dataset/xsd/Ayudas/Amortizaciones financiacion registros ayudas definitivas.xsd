    <element ref='t:IMPORTECAPITAL' minOccurs='0' maxOccurs='1'/>
    <element ref='t:IMPORTEINTERESES' minOccurs='0' maxOccurs='1'/>
    <element ref='t:IMPORTE' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDDOCUMENTO' type='string'>
 </element>

 <element name='NUMEROLINEAFINANCIACION' type='string'>
 </element>

 <element name='CODIGOTIPOFINANCIACION' type='string'>
