    <element ref='t:IDTERCEROCONVOCANTE' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHACONVOCATORIA' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHARESOLUCION' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHAPUBLICACION' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDDOCUMENTO' type='string'>
 </element>

 <element name='IDCONVOCATORIARECURSOHUMANO' type='string'>
 </element>

 <element name='DESCRIPCIONCONVOCATORIA' type='string'>
