    <element ref='t:IDEMPRESAFINANCIADORA'/>
    <element ref='t:REFERENCIACONVOCATORIA' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHACONVOCATORIA'/>
    <element ref='t:FECHAALEGACION' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHAPUBLICACIONBOLETIN' minOccurs='0' maxOccurs='1'/>
    <element ref='t:OBSERVACIONES' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDCONVOCATORIARECURSOHUMANO' type='string'>
 </element>

 <element name='DESCRIPCION' type='string'>
 </element>

 <element name='IDEMPRESAFINANCIADORA' type='string'>
 </element>

 <element name='REFERENCIACONVOCATORIA' type='string'>
 </element>

 <element name='FECHACONVOCATORIA' type='string'>
 </element>

 <element name='FECHAALEGACION' type='string'>
 </element>

 <element name='FECHAPUBLICACIONBOLETIN' type='string'>
 </element>

 <element name='OBSERVACIONES' type='string'>
 </element>
</schema>
