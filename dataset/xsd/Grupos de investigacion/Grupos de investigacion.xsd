    <element ref='t:CODUNIDADADM' minOccurs='0' maxOccurs='1'/>
    <element ref='t:EXCELENCIA' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHACREACION' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FECHADESAPARICION' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

 <element name='IDGRUPOINVESTIGACION' type='string'>
 </element>

 <element name='DESCRIPCION' type='string'>
 </element>

 <element name='CODUNIDADADM' type='string'>
 </element>

 <element name='EXCELENCIA' type='string'>
 </element>

 <element name='FECHACREACION' type='string'>
 </element>

 <element name='FECHADESAPARICION' type='string'>
 </element>
</schema>
